/*打印模版*/
.print{
	padding: 50px;
}
.print table{
	width:60%;
}
.table .name, .table .size, .table .padd, .table .start{
	margin-top: 5px;
}
.print>table>tbody>tr>td:first-child{
	background: #F5FAFE;
}
table .name>span:nth-of-type(1),
table .size>span:nth-of-type(1),
table .padd>span:nth-of-type(1),
table .start>span:nth-of-type(1){
	display: inline-block;
	width:150px;
	text-align: right;
}
table .name>input{
	border: 1px solid #ddd;
	box-sizing: border-box;
	width: 50%;
	-webkit-border-radius: 2px;
	-moz-border-radius: 2px;
	border-radius: 2px;
	padding: 4px 8px;
}
table .name>.btn-pink{
	margin-left: 10px;
}
table .size input{
	border: 1px solid #ddd;
	box-sizing: border-box;
	width: 20%;
	-webkit-border-radius: 2px;
	-moz-border-radius: 2px;
	border-radius: 2px;
	padding: 4px 8px;
	margin:0 10px;
}
table .padd input{
	border: 1px solid #ddd;
	box-sizing: border-box;
	width: 15%;
	-webkit-border-radius: 2px;
	-moz-border-radius: 2px;
	border-radius: 2px;
	padding: 4px 8px;
	margin:0 10px;
}
table .start input{
	width: 16px;
	height: 16px;
	box-sizing: border-box;
	margin:-1px 5px 0;
}
.view .col-md-9{
	border: 1px solid #dcdada;
	padding: 20px;
	background: #f5f5f5;
}
.view .table-name>input{
	width:100%;
	border: 1px solid #ddd;
	box-sizing: border-box;
	-webkit-border-radius: 2px;
	-moz-border-radius: 2px;
	border-radius: 2px;
	padding: 4px 8px;
	margin:8px 0;
	text-align: center;
}
.view .num input,.view .total input,.view .nums input{
	width:30%;
	border: 1px solid #ddd;
	box-sizing: border-box;
	-webkit-border-radius: 2px;
	-moz-border-radius: 2px;
	border-radius: 2px;
	padding: 4px 8px;
	margin:8px 0;
	text-align: center;
}
.view .num>span:nth-of-type(1),
.view .total>span:nth-of-type(1),
.view .nums>span:nth-of-type(1) {
	display: inline-block;
	width: 100px;
	text-align: right;
}
